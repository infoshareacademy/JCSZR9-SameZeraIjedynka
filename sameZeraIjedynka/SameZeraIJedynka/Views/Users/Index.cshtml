

@model List<SameZeraIjedynka.Database.Entities.User>

@{
}

<h1>Users</h1>
<table class="table">
    <thread>
        <tr>
            
            <th>Name</th>

            <th> </th>
        </tr>
    </thread>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name</td>
                <td><a href="Users/View/@user.UserId">View</a></td>
            </tr>
        }
    </tbody>
</table>
